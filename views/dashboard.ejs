        $(document).ready(function(){
            $("#printArea").fadeOut(0);
            $("#lineBuzzer").hide();

            $.post("./config/data", function(data){
                console.log(data);
                if(data.result==1){
                    $("#billAmount").val(data.info.numberPrint);
                    var notes = data.info.note.split(";");
                    if(notes.length>0){
                        notes.forEach(function(item){
                            $("#tags").append(`<option value="`+item+`">`);
                        });
                    }
                }
            });

            $("#txtSearchFood").keyup(function(){
                var txt = $(this).val().trim();
                if(txt.length>0){

                }
            });
        });

        function printOrder(idOrder, currentButton){ // currentButton=1  TIEN MAT (CASH)
            $.post("./config/data", function(data){
                console.log(data);
                if(data.result==1){
                    $("#logonvoid").attr("src", "./" + data.info.logo);
                    $("#line1").html(data.info.line1);
                    $("#line2").html(data.info.line2);
                    $("#line3").html(data.info.line3);
                    $("#lineABN").html(data.info.abn);
                    $("#printArea").fadeIn(0);
                    //var idOrder = $(this).attr("_idorder");
                    $.post("./orders/detail", {idOrder:idOrder}, function(data){

                        console.log(data);
                        
                        if(data.result==1){
                            var date = new Date(data.order.dateCreated);  // dateStr you get from mongodb
                            console.log(date.getMonth());
                            console.log(date.getDate());
                            var times =date.getFullYear()  + "/" + (date.getMonth()+1) + "/" + (parseInt(date.getDate())) + " " + date.getHours() + ":" + date.getMinutes() + ":" + date.getSeconds();
                            
                            var pickup = "";
                            if(data.order.pickup_hour>-1){
                                var hour = data.order.pickup_hour;
                                if(hour>=0 && hour<=9){
                                    hour = "0" + hour;
                                }
                                var min = data.order.pickup_minute;
                                if(min>=0 && min<=9){
                                    min = "0" + min;
                                }
                                pickup += "("+hour+":"+min+")"
                            }

                            if(data.order.orderType==3){
                                $("#nameCus").html(data.order.customerName + " " + pickup);
                                $("#rowCash").hide();
                                $("#rowCharge").hide();
                            }else{
                                $("#nameCus").hide();
                            }

                            if(data.order.orderType==1){
                                $("#lineBuzzer").show();
                                $("#buzzer").html(data.order.customerName);
                            }
                            
                            $("#orderDate").html(times);
                            $("#txtNote").html(data.order.note);

                            $("#orderCode").html(data.order.orderCode);
                            $("#totalPriceAllOrder").html("$" + data.order.totalOrders_Final.toFixed(2));
                            $("#totalOrders_Discount").html("$" + data.order.totalOrders_Discount.toFixed(2));

                            if(currentButton==1){
                                $("#totalOrders_onCash").html("$" + data.order.moneyAmount_customer.toFixed(2));
                                $("#totalOrders_onCharge").html("$" + parseFloat(data.order.moneyAmount_back.toFixed(2)).toFixed(2));
                            }

                            if(parseInt(data.order.totalOrders_Discount)==0){
                                $(".row_totalOrders_Discount").hide();
                            }


                            $("#tblOrders").html(`<tr class="row1">
                                <td>Item</td>
                                <td style="text-align:center">$</td>
                                <td style="text-align:center">Qty</td>
                                <td style="text-align:center">$</td>
                            </tr>`);

                            var totalTaxable = 0;
                            var totalGST = 0;
                            var totalNonTaxable = 0;

                            data.order.categoryList.forEach(function(item){
                                //var price = item.amount * (item.price - (item.price/100*item.discount));
                                
                                // totalTaxable: tong gia tri chua thue cua food co GST
                                if(parseInt(item.tax)>0){
                                    totalGST += item.taxCounting*item.amount;                            
                                    totalTaxable += item.lastPrice*item.amount - item.taxCounting*item.amount;
                                }

                                var strOptions = "";
                                item.options.forEach(function(op){
                                    if(parseInt(op.tax)>0){
                                        totalTaxable += op.price*item.amount;
                                        totalGST += op.price/100*parseInt(op.tax)*item.amount;  
                                    }
                                    if(parseInt(op.tax)==0){
                                        totalNonTaxable += op.price*item.amount;
                                    }
                                    strOptions += "<div>" + op.name + " $" + op.fullPrice + "<div>"
                                });
                                // totalGST: tong GST cua totalTaxable

                                // total Non taxable: tong gia tri chua thue cua food KHONG GST
                                if(item.tax==null){
                                    totalNonTaxable += item.price*item.amount;
                                }
                                $("#totalNonTaxable").html("$" + totalNonTaxable.toFixed(2));
                                // totalPriceOrder: totalOrders_Final

                                

                                $("#tblOrders").append(`
                                <tr>
                                    <td>
                                        <div class="food"><span style="font-weight:bold">
                                        ` + item.name + "</span><div style='padding-left:15px'>" + strOptions +`    
                                        </div></div>
                                        <div class="option"></div>
                                    </td>
                                    
                                    <td style="text-align:center">
                                        `+item.lastPrice.toFixed(2)+`  
                                    </td>
                                    <td style="text-align:center" class="todam">
                                        `+item.amount+`  
                                    </td>
                                    <td style="text-align:center">
                                        `+(item.priceInOrder.toFixed(2))+`  
                                    </td>
                                </tr>
                                `);
                            });

                            $("#totalGST").html("$" +  totalGST.toFixed(2));
                            $("#totalTaxable").html("$" + totalTaxable.toFixed(2));

                            $("#printArea").print();
                            //window.print();
                            $("#printArea").fadeOut(0);
                        }
                    });
                }else{
                    alert(data.message);
                }
            });

            
        }
    </script>
